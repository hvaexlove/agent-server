{
  "name": "agent-server",
  "version": "1.0.0",
  "description": "agent-server - open source server",
  "main": "main.ts",
  "scripts": {
    "start": "node dist/main.js -c config.yml",
    "serve": "nodemon dist/main.js",
    "build": "npm run build-ts",
    "build-tslint": "npm run tslint && npm run build-ts",
    "build-ts": "tsc",
    "watch": "npm run watch-ts",
    "watch-tslint": "npm run tslint && npm run watch-ts",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "pkg:dev": "node build/dev.js",
    "repkg": "npm run build && npm run pkg:dev"
  },
  "dependencies": {
    "@types/node": "^10.12.9",
    "@types/koa-router": "^7.0.32",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "log4js": "^3.0.5",
    "mysql": "^2.16.0",
    "node-uuid": "^1.4.8",
    "pkg": "^4.3.4",
    "require-yml": "^1.3.1",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "tslint": "^5.10.0",
    "typescript": "^3.0.1"
  },
  "author": "shaoshuai",
  "license": "ISC",
  "readmeFilename": "README.md"
}
